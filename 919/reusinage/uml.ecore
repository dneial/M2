<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="uml" nsURI="uml" nsPrefix="p">
  <eClassifiers xsi:type="ecore:EClass" name="Animal">
    <eOperations name="isMammal"/>
    <eOperations name="mate"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="age" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="gender" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Duck" eSuperTypes="#//Animal">
    <eOperations name="swim"/>
    <eOperations name="quack"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="beakColor" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        defaultValueLiteral="yellow"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Fish" eSuperTypes="#//Animal">
    <eOperations name="swim"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="canEat" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
  </eClassifiers>
</ecore:EPackage>
